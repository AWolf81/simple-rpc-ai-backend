# PostgreSQL Secret Manager Configuration
# Copy this file to .env.postgres and update with your values
# .env.postgres is gitignored and will not be committed

# Database Configuration
POSTGRES_SECRET_PASS=your-secure-postgres-password-here

# Secret Manager Configuration  
SECRET_MANAGER_ENCRYPTION_KEY=your-32-character-encryption-key-here

# Database Connection
SECRET_MANAGER_DB_HOST=localhost
SECRET_MANAGER_DB_PORT=5432
SECRET_MANAGER_DB_NAME=secrets
SECRET_MANAGER_DB_USER=secret_manager
SECRET_MANAGER_DB_PASS=your-secure-postgres-password-here