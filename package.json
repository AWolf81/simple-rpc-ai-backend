{
  "name": "simple-rpc-ai-backend",
  "version": "0.1.4",
  "description": "Simple RPC client and server for AI-powered VS Code extensions with secure system prompt management",
  "type": "module",
  "pnpm": {
    "overrides": {
      "nanoid": "^3.3.8",
      "esbuild": "^0.25.0",
      "loader-utils": ">=1.4.1",
      "json-schema": ">=0.4.0",
      "semantic-release": ">=17.2.3",
      "trim": ">=0.0.3",
      "semver-regex": ">=3.1.3",
      "marked": ">=4.0.10",
      "node-fetch": ">=2.6.7",
      "json5": ">=1.0.2",
      "cross-spawn": ">=6.0.6",
      "lodash.set": ">=4.3.3",
      "hono": ">=4.9.7",
      "postcss": ">=8.4.31",
      "@octokit/request-error": ">=5.1.1",
      "vite": "^6.3.6",
      "@octokit/request": ">=8.4.1",
      "@octokit/plugin-paginate-rest": ">=9.2.2",
      "prismjs": ">=1.30.0",
      "babel-traverse": "npm:@babel/traverse@^7.23.2",
      "html-parse-stringify2": "npm:html-parse-stringify@^3.0.1",
      "tar-fs": ">=3.1.1",
      "jsondiffpatch": ">=0.7.2"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom",
        "@trpc/server"
      ],
      "allowedVersions": {
        "trpc-panel>@trpc/server": "11"
      }
    },
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "engines": {
    "node": ">=22.18.0",
    "pnpm": ">=7.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "simple-rpc-dev-panel": "./dist/tools/dev-panel.js",
    "simple-rpc-gen-methods": "./dist/tools/bin/generate-trpc-methods.js",
    "check-mcp-security": "./dist/tools/check-mcp-security.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "default": "./dist/server.js"
    },
    "./ai": {
      "types": "./dist/ai-service.d.ts",
      "default": "./dist/ai-service.js"
    },
    "./dev-panel": {
      "types": "./dist/tools/dev-panel-api.d.ts",
      "default": "./dist/tools/dev-panel-api.js"
    }
  },
  "scripts": {
    "build": "npm run copy-assets && tsc && tsc-alias && npm run build:base",
    "build:base": "TRPC_GEN_SYSTEM_ENABLED=true TRPC_GEN_USER_ENABLED=true TRPC_GEN_BILLING_ENABLED=true TRPC_GEN_AUTH_ENABLED=true TRPC_GEN_ADMIN_ENABLED=true node tools/generate-trpc-methods.js",
    "build:mcp": "npm run copy-assets && tsc && tsc-alias && TRPC_GEN_CUSTOM_ROUTERS=examples/02-mcp-server/methods/index.js node tools/generate-trpc-methods.js",
    "build:basic": "npm run copy-assets && tsc && tsc-alias && TRPC_GEN_CUSTOM_ROUTERS=examples/01-basic-server/server.js node tools/generate-trpc-methods.js",
    "build:prod": "npm run build",
    "postinstall": "test -d .git && node scripts/safe-build.js || echo 'Skipping build (installed package)'",
    "prepare": "test -d .git && (node scripts/safe-build.js && husky) || echo 'Skipping prepare (installed package)'",
    "release:next": "semantic-release --branches next",
    "release": "semantic-release",
    "build:trpc-methods": "node tools/generate-trpc-methods.js",
    "copy-assets": "mkdir -p dist/docker dist/database/migrations dist/views dist/data dist/tools dist/tools/bin && cp -r docker/* dist/docker/ 2>/dev/null || true && cp docker-compose.postgres.yml dist/ 2>/dev/null || true && cp .env.postgres.example dist/ 2>/dev/null || true && cp src/database/migrations/*.sql dist/database/migrations/ 2>/dev/null || true && cp -r src/views/* dist/views/ 2>/dev/null || true && cp -r src/data/* dist/data/ 2>/dev/null || true && cp tools/dev-panel.js dist/tools/dev-panel.js 2>/dev/null || true && cp tools/trpc-playground-fix.js dist/tools/trpc-playground-fix.js 2>/dev/null || true && cp tools/generate-trpc-methods.js dist/tools/generate-trpc-methods.js 2>/dev/null || true && cp tools/bin/generate-trpc-methods.js dist/tools/bin/generate-trpc-methods.js 2>/dev/null || true && chmod +x dist/tools/dev-panel.js dist/tools/generate-trpc-methods.js dist/tools/bin/generate-trpc-methods.js dist/tools/check-mcp-security.js 2>/dev/null || true",
    "mcp:check": "tsx src/tools/check-mcp-security.ts",
    "mcp:scan": "tsx src/tools/check-mcp-security.ts",
    "dev": "tsc --watch",
    "dev:tsx": "tsx --watch src/index.ts",
    "dev:server": "tsx --watch examples/02-mcp-server/server.js",
    "dev:server:basic": "tsx --watch examples/01-basic-server/server.js",
    "dev:server:tasks": "tsx --watch examples/04-mcp-tasks-server/server.js",
    "dev:server:resources": "tsx --watch examples/05-local-resources-server/server.js",
    "dev:panel": "node tools/dev-panel.js --server-port ${AI_SERVER_PORT:-8001}",
    "dev:panel:basic": "node tools/dev-panel.js --server-port 8000",
    "dev:mcpjam": "node ./node_modules/@mcpjam/inspector/bin/start.js --port 4000",
    "dev:playground": "pnpm dev:panel",
    "dev:docs": "concurrently \"pnpm dev:server\" \"pnpm dev:playground\"",
    "dev:docs:basic": "concurrently \"pnpm dev:server:basic\" \"pnpm dev:panel:basic\"",
    "dev:full": "concurrently \"pnpm dev\" \"pnpm dev:server\" \"pnpm dev:playground\"",
    "dev:full:basic": "concurrently \"pnpm dev\" \"pnpm dev:server:basic\" \"pnpm dev:playground\"",
    "start": "node examples/02-mcp-server/server.js",
    "start:basic": "node examples/01-basic-server/server.js",
    "start:mcp": "node examples/02-mcp-server/server.js",
    "start:tasks": "node examples/04-mcp-tasks-server/server.js",
    "start:resources": "node examples/05-local-resources-server/server.js",
    "kill-ports": "lsof -ti:8000,8001,8080,3000,4000,8090 | xargs kill -9 2>/dev/null || echo 'âœ… No processes found on development ports'",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:integration": "node test/integration/index.js",
    "test:manual": "tsx src/test/**/*.test.ts",
    "test:isolation": "tsx src/test/postgres/user-isolation.test.ts",
    "test:postgres": "tsx src/test/postgres/user-isolation.test.ts",
    "tsx": "tsx",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "convert-aliases": "node tools/convert-to-aliases.js",
    "clean": "rimraf dist coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepublishOnly": "npm run build",
    "jekyll:setup": "cd docs && ruby -S bundle install",
    "docs:methods": "node tools/generate-method-docs.js",
    "jekyll:build": "TRPC_GEN_INCLUDE_ALL=true pnpm build:trpc-methods && pnpm docs:methods && cd docs && bundle exec jekyll build --destination _site",
    "jekyll:dev": "TRPC_GEN_INCLUDE_ALL=true pnpm build:trpc-methods && pnpm docs:methods && cd docs && bundle exec jekyll serve --port 8090 --livereload --watch",
    "jekyll:publish": "pnpm jekyll:build && gh-pages -d docs/_site -b gh-pages -m \"docs: publish\"",
    "jekyll:deploy": "pnpm jekyll:publish",
    "validate:openrpc": "npx @open-rpc/schema-utils-js validate --document http://localhost:8000/openrpc.json",
    "postgres:setup": "./docker/setup-postgres.sh",
    "postgres:start": "docker-compose --env-file .env.postgres -f docker-compose.postgres.yml up -d",
    "postgres:stop": "docker-compose --env-file .env.postgres -f docker-compose.postgres.yml down",
    "postgres:logs": "docker-compose --env-file .env.postgres -f docker-compose.postgres.yml logs -f",
    "demo:storage": "node examples/flexible-storage-examples.js",
    "demo:trpc": "node -e \"import('./dist/server-trpc.js').then(({createTRPCServer}) => { const server = createTRPCServer({port: 8001}); server.start().then(() => console.log('ðŸš€ tRPC demo running on http://localhost:8001\\nTry: curl http://localhost:8001/health')); })\"",
    "demo:oauth": "pnpm build && node examples/authentication/basic-oauth-example.js",
    "demo:oauth:branded": "pnpm build && node examples/authentication/branded-oauth-example.js",
    "demo:mcp:opensaas": "pnpm build && node examples/authentication/opensaas-mcp-auth-example.js",
    "test:oauth": "node test-oauth-discovery.js",
    "test:trpc": "node -e \"import('./dist/server-trpc.js').then(async ({createTRPCServer}) => { const s = createTRPCServer({port: 8003}); await s.start(); const r = await fetch('http://localhost:8003/health'); console.log('âœ… tRPC test:', (await r.json()).status); await s.stop(); })\"",
    "install:safe": "node scripts/install-with-fallback.js"
  },
  "keywords": [
    "vscode",
    "extension",
    "rpc",
    "ai",
    "anthropic",
    "openai",
    "code-analysis",
    "backend",
    "jsonrpc",
    "secure"
  ],
  "author": "Alexander Wolf <AWolf81>",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.23",
    "@ai-sdk/google": "^2.0.17",
    "@ai-sdk/openai": "^2.0.42",
    "@anolilab/ai-model-registry": "^1.3.0",
    "@huggingface/inference": "^4.8.0",
    "@modelcontextprotocol/sdk": "^1.17.4",
    "@node-oauth/express-oauth-server": "^4.1.4",
    "@node-oauth/oauth2-server": "^5.2.1",
    "@trpc/client": "^11.4.4",
    "@trpc/server": "^11.4.4",
    "@tsconfig/node22": "^22.0.2",
    "ai": "^4.0.0",
    "axios": "^1.12.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "express-handlebars": "^8.0.3",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.2",
    "fast-xml-parser": "^4.3.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.7.0",
    "json-rpc-2.0": "^1.7.1",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.0",
    "openid-client": "^6.7.1",
    "pg": "^8.16.3",
    "redis": "^5.7.0",
    "superjson": "^2.2.2",
    "trpc-panel": "^1.3.4",
    "trpc-playground": "^1.0.4",
    "trpc-to-mcp": "^1.3.2",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "zod": "^4.1.12"
  },
  "optionalDependencies": {
    "@mcpjam/inspector": "^0.9.8",
    "@testcontainers/postgresql": "^10.2.0"
  },
  "devDependencies": {
    "@opentelemetry/core": "^2.0.1",
    "@tsconfig/node24": "^24.0.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.0.0",
    "@types/node-fetch": "^2.6.4",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "cheerio": "^1.1.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.0.0",
    "gh-pages": "^6.1.1",
    "git-branch-is": "^4.0.0",
    "husky": "^9.1.7",
    "lodash": "^4.17.21",
    "node-fetch": ">=2.6.7",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "semantic-release": "^19.0.5",
    "supertest": "^7.1.4",
    "ts-morph": "^27.0.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.4",
    "typescript": "^5.0.0",
    "typescript-transform-paths": "^3.5.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^1.0.0",
    "zod-to-ts": "^1.2.0"
  },
  "files": [
    "dist",
    "tools",
    "scripts",
    "README.md",
    "LICENSE.md",
    "docker/**/*",
    "docker-compose.postgres.yml",
    ".env.postgres.example"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AWolf81/simple-rpc-ai-backend"
  }
}
